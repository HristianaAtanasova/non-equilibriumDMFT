\begin_layout Subsubsection
Green's function computation within the non-crossing approximation 
\end_layout

\begin_layout Subsubsection*
Bold propagators 
\end_layout

\begin_layout Standard
The first step is to compute so called bold propagators 
\begin_inset Formula $G_{\alpha\beta}(t)$
\end_inset

 between many the body states 
\begin_inset Formula $\alpha$
\end_inset

 and 
\begin_inset Formula $\beta$
\end_inset

 containing all hybridization lines in a given time intervall that do not
 cross each other (therefore the name non-crossing approximation) and exist
 on one branch only.
 Propagators on the upper and lower branch obey the relation 
\begin_inset Formula $G_{\alpha\beta}^{\dagger}(t)=G_{\alpha\beta}(\overline{t}).$
\end_inset

 Starting from the formal expression
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{\alpha\beta}(t)=\langle\langle\alpha\mid\rho_{D}exp^{-iHt}\mid\beta\rangle\rangle_{B}=\langle\langle\alpha\mid\rho_{D}\exp^{-iH_{0}t}U(t)\mid\beta\rangle\rangle_{B},
\]

\end_inset

where 
\begin_inset Formula $\langle\cdots\rangle_{B}=Tr\left\{ \rho_{B}\cdots\right\} $
\end_inset

 denotes that the bath degrees are traced out, the expansion for the time
 evolution operator 
\begin_inset Formula $U(t)$
\end_inset

 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:expansion_for_U"
plural "false"
caps "false"
noprefix "false"

\end_inset

 can be inserted
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{\alpha\alpha}(t)=\langle\langle\alpha\mid\rho_{D}\exp^{-iH_{0}t}\mid\alpha\rangle\rangle_{B}+(-i)^{2}\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}\langle\langle\alpha\mid\rho_{D}\exp^{-iH_{0}t}\widehat{H}_{hyb}(t_{1})\widehat{H}_{hyb}(t_{2})\mid\alpha\rangle\rangle_{B}+\cdots
\]

\end_inset


\begin_inset Formula 
\begin{multline*}
=\sum_{\beta}\langle\langle\alpha\mid\beta\rangle\langle\beta\mid\exp^{-iH_{0}t}\mid\alpha\rangle\rangle_{B}-\sum_{\beta}\sum_{\sigma,\lambda}\int dt_{1}\int_{0}^{t_{1}}dt_{2}\left|t_{\sigma\lambda}\right|^{2}\times\\
\left(\langle\langle\alpha\mid\exp^{-iH_{0}t}d_{\sigma}\mid\beta\rangle\langle\beta\mid d_{\sigma}^{\dagger}\hat{b}_{\sigma\lambda}^{\dagger}(t_{1})\hat{b}_{\sigma\lambda}(t_{2})\mid\alpha\rangle\rangle_{B}+\langle\langle\alpha\mid\exp^{-iH_{0}t}d_{\sigma}^{\dagger}\mid\beta\rangle\langle\beta\mid d_{\sigma}\hat{b}_{\sigma\lambda}(t_{2})\hat{b}_{\sigma\lambda}^{\dagger}(t_{1})\mid\alpha\rangle\rangle_{B}\right)+\cdots
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
\mathrm{=\delta_{\alpha\beta}exp^{-i\varepsilon_{\alpha}t}-\sum_{\beta}\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\alpha\alpha}^{(0)}(t-t_{1})G_{\beta\beta}^{(0)}(t_{1}-t_{2})\times}\\
\left(\langle\alpha\mid d_{\sigma}\mid\beta\rangle\langle\beta\mid d_{\sigma}^{\dagger}\mid\alpha\rangle\sum_{\sigma,\lambda}\left|t_{\sigma\lambda}\right|^{2}\langle\hat{b}_{\sigma\lambda}^{\dagger}(t_{1})\hat{b}_{\sigma\lambda}(t_{2})\rangle_{B}+\langle\alpha\mid d_{\sigma}^{\dagger}\mid\beta\rangle\langle\beta\mid d_{\sigma}\mid\alpha\rangle\sum_{\sigma,\lambda}\left|t_{\sigma\lambda}\right|^{2}\langle\hat{b}_{\sigma\lambda}(t_{2})\hat{b}_{\sigma\lambda}^{\dagger}(t_{1})\rangle_{B}\right)G_{\alpha\alpha}(t_{2})+\cdots
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
=G_{\alpha\alpha}^{(0)}(t)-\sum_{\beta}\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\alpha\alpha}^{(0)}(t-t_{1})G_{\beta\beta}^{(0)}(t_{1}-t_{2})\times\\
\left(\langle\alpha\mid d_{\sigma}\mid\beta\rangle\langle\beta\mid d_{\sigma}^{\dagger}\mid\alpha\rangle\bigtriangleup_{\sigma}^{<}(t_{1}-t_{2})+\langle\alpha\mid d_{\sigma}^{\dagger}\mid\beta\rangle\langle\beta\mid d_{\sigma}\mid\alpha\rangle\bigtriangleup_{\sigma}^{>}(t_{1}-t_{2})\right)G_{\alpha\alpha}^{(0)}(t_{2})+\cdots
\end{multline*}

\end_inset

with the bare atomic state propagators 
\begin_inset Formula $G_{\alpha\alpha}^{(0)}(t)=\exp^{-i\varepsilon_{\alpha}t}$
\end_inset

 leaving the dot state invariant.
 Both the bare 
\begin_inset Formula $G_{\alpha\alpha}^{(0)}(t)$
\end_inset

 and bold propagator 
\begin_inset Formula $G_{\alpha\alpha}(t)$
\end_inset

 are diagonal for the Anderson impurity model because the trace over the
 bath differs from zero only for an even number of creation and annhilation
 operators, therefore only square terms of 
\begin_inset Formula $\widehat{H}_{hyb}$
\end_inset

 appear in the expansion for the propagators.
 In DMFT the hybridization function 
\begin_inset Formula $\Delta_{\sigma}^{>/<}$
\end_inset

 is obtained from the self-consistency condition, but for the initial guess
 it can be expressed in terms of the coupling density 
\begin_inset Formula $\Gamma(\omega)=\pi\sum_{k}\mid t_{k}\mid^{2}\delta(\omega-\varepsilon_{k})$
\end_inset

 that fully describes the bath properties and the hybridization with the
 initial Fermi Dirac distribution 
\begin_inset Formula $f(\omega-\mu)=\frac{1}{1+\exp^{\beta(\omega-\mu)}}$
\end_inset


\begin_inset Formula 
\begin{equation}
\Delta^{<}(t_{1},t_{2})=\int_{-\infty}^{\infty}\frac{d\omega}{\pi}exp^{-i\omega(t_{1}-t_{2})}\varGamma(\omega)f(\omega-\mu)\label{eq:Delta}
\end{equation}

\end_inset

and
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Delta^{>}(t_{1},t_{2})=\int_{-\infty}^{\infty}\frac{d\omega}{\pi}exp^{-i\omega(t_{1}-t_{2})}\varGamma(\omega)(1-f(\omega-\mu)).
\]

\end_inset

With the approximation for the self-energy, that it includes only non-crossing
 hybridizaton lines, the Dyson equation can be written as
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{\alpha\alpha}(t)=G_{\alpha\alpha}^{(0)}(t)-\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\alpha\alpha}^{(0)}(t-t_{1})\varSigma_{\alpha\alpha}(t_{1}-t_{2})G_{\alpha\alpha}(t_{2})
\]

\end_inset

with
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
\varSigma_{\alpha\alpha}(t_{1}-t_{2})=\sum_{\sigma}\sum_{\beta}G_{\beta\beta}(t_{1}-t_{2})\times\\
\left(\langle\alpha\mid d_{\sigma}\mid\beta\rangle\langle\beta\mid d_{\sigma}^{\dagger}\mid\alpha\rangle\Delta_{\sigma}^{<}(t_{1}-t_{2})+\langle\alpha\mid d_{\sigma}^{\dagger}\mid\beta\rangle\langle\beta\mid d_{\sigma}\mid\alpha\rangle\Delta_{\sigma}^{>}(t_{1}-t_{2})\right).
\end{multline*}

\end_inset

For the four different initial states, denoted by the outer index 
\begin_inset Formula $\alpha$
\end_inset

, on the dot a set of coupled Dyson equations is derived
\begin_inset Formula 
\begin{multline*}
G_{0}(t)=G_{0}^{(0)}(t)-\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{0}^{(0)}(t-t_{1})G_{\uparrow}(t_{1}-t_{2})\Delta_{\uparrow}^{<}(t_{1}-t_{2})G_{0}(t_{2})-\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{0}^{(0)}(t-t_{1})G_{\downarrow}(t_{1}-t_{2})\Delta_{\downarrow}^{<}(t_{1}-t_{2})G_{0}(t_{2}})
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
G_{\uparrow}(t)=G_{\uparrow}^{(0)}(t)-\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\uparrow}^{(0)}(t-t_{1})G_{0}(t_{1}-t_{2})\Delta_{\uparrow}^{>}(t_{1}-t_{2})G_{\uparrow}(t_{2})-\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\uparrow}^{(0)}(t-t_{1})G_{\uparrow\downarrow}(t_{1}-t_{2})\Delta_{\downarrow}^{<}(t_{1}-t_{2})G_{\uparrow}(t_{2})}
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
G_{\downarrow}(t)=G_{\downarrow}^{(0)}(t)-\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\downarrow}^{(0)}(t-t_{1})G_{0}(t_{1}-t_{2})\Delta_{\uparrow}^{>}(t_{1}-t_{2})G_{\downarrow}(t_{2})-\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\downarrow}^{(0)}(t-t_{1})G_{\uparrow\downarrow}(t_{1}-t_{2})\Delta_{\downarrow}^{<}(t_{1}-t_{2})G_{\downarrow}(t_{2})}
\end{multline*}

\end_inset


\begin_inset Formula 
\begin{multline*}
G_{\uparrow\downarrow}(t)=G_{\uparrow\downarrow}^{(0)}(t)-\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\uparrow\downarrow}^{(0)}(t-t_{1})G_{\downarrow}(t_{1}-t_{2})\Delta_{\uparrow}^{>}(t_{1}-t_{2})G_{\uparrow\downarrow}(t_{2})-\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t_{1}}dt_{2}G_{\uparrow\downarrow}^{(0)}(t-t_{1})G_{\uparrow}(t_{1}-t_{2})\Delta_{\downarrow}^{>}(t_{1}-t_{2})G_{\uparrow\downarrow}(t_{2}),}
\end{multline*}

\end_inset

which can be solved by a self consistent iteration.
 In equilibirum all entities depend only on time differences and one can
 evaluate the integrals in the Fourier space, where convolutions correspond
 to multiplications.
 Solving these above equations for non-equilibirum system is more complex
 and will be treated in the next chapter.
 Before starting with the first iteration, the bold propagators 
\begin_inset Formula $\mathrm{G_{\alpha\alpha}(t)}$
\end_inset

 are initialized by the bare propagators 
\begin_inset Formula $\mathrm{G_{\alpha\alpha}^{(0)}(t)}$
\end_inset

 from which the first self-energy is obtained.
 Both are inserted into the Dyson equation with the interaction times integrated
 over.
 With the updated 
\begin_inset Formula $\mathrm{G_{\alpha\alpha}(t)}$
\end_inset

 one can go back to the second step and compute a new self energy for the
 next iteration cycle until convergence is reached.
 
\end_layout

\begin_layout Subsubsection*
Correlation functions 
\end_layout

\begin_layout Standard
So far we have calculated propagators between many body states, which contain
 all non-crossing hybridization lines in a time segment on a single branch.
 These objects have no physical meaning and in torder to compute physical
 observables it is necessary to include hybridization lines connecting times
 on both branches.
 We introduce so called vertex functions 
\begin_inset Formula $\mathrm{K_{\alpha\beta}(t,t^{\prime})}$
\end_inset

 with the first
\begin_inset Formula $\mathrm{[second]}$
\end_inset

 time index on the upper
\begin_inset Formula $\mathrm{[lower]}$
\end_inset

 branch.
 A vertex funcion incorporates all non-crossing intra- and inter-branch
 hybrdization lines.
 The procedure to estimate 
\begin_inset Formula $\mathrm{K_{\alpha\beta}(t,t^{\prime})}$
\end_inset

 is similar to the calculation of 
\begin_inset Formula $\mathrm{G_{\alpha\alpha}(t)}$
\end_inset

 with the difference that 
\begin_inset Formula $\mathrm{K_{\alpha\beta}(t,t^{\prime})}$
\end_inset

 is a two-times function and instead of four we have 16 equations representing
 the evolution of the dot from any inital state 
\begin_inset Formula $\alpha$
\end_inset

 to any final state 
\begin_inset Formula $\beta$
\end_inset

.
 For every inital state there are four coupled equations of the form 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
K_{\alpha\beta}(t,t^{\prime})=K_{\alpha\beta}^{(0)}(t,t^{\prime})+\sum_{\gamma\delta}\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}K_{\alpha\gamma}(t_{1},t_{2})\mathbf{\Delta}_{\gamma\delta}(t_{1},t_{2})G_{\delta\beta}^{\dagger}(t-t_{1})G_{\delta\beta}(t^{\prime}-t_{2}).
\]

\end_inset

The vertex functions without inter-branch hybridization lines are composed
 by the bold propagators on the upper and lower branch
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
K_{\alpha\beta}^{(0)}(t,t^{\prime})=\delta_{\alpha\beta}G_{\alpha\beta}^{\dagger}(t)G_{\alpha\beta}(t^{\prime}).
\]

\end_inset

The self consistent equations for the initially unoccupied dot are
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
K_{00}(t,t^{\prime})=K_{00}^{(0)}(t,t^{\prime})+\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{0}^{\dagger}(t^{\prime}-t_{2})G_{0}(t-t_{1})\Delta_{\uparrow}^{<}(t_{1},t_{2})K_{0\uparrow}(t_{1},t_{2})+\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{0}^{\dagger}(t^{\prime}-t_{2})G_{0}(t-t_{1})\Delta_{\downarrow}^{<}(t_{1},t_{2})K_{0\downarrow}(t_{1},t_{2})}
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
K_{0\uparrow}(t,t^{\prime})=\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{\uparrow}^{\dagger}(t^{\prime}-t_{2})G_{\uparrow}(t-t_{1})\Delta_{\uparrow}^{>}(t_{1},t_{2})K_{00}(t_{1},t_{2})+\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{\uparrow}^{\dagger}(t^{\prime}-t_{2})G_{\uparrow}(t-t_{1})\Delta_{\downarrow}^{<}(t_{1},t_{2})K_{0\uparrow\downarrow}(t_{1},t_{2})}
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
K_{0\downarrow}(t,t^{\prime})=\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{\downarrow}^{\dagger}(t^{\prime}-t_{2})G_{\downarrow}(t-t_{1})\Delta_{\downarrow}^{>}(t_{1},t_{2})K_{00}(t_{1},t_{2})+\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{\downarrow}^{\dagger}(t^{\prime}-t_{2})G_{\downarrow}(t-t_{1})\Delta_{\uparrow}^{<}(t_{1},t_{2})K_{0\uparrow\downarrow}(t_{1},t_{2})}
\end{multline*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{multline*}
K_{0\uparrow\downarrow}(t,t^{\prime})=\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{\uparrow\downarrow}^{\dagger}(t^{\prime}-t_{2})G_{\uparrow\downarrow}(t-t_{1})\Delta_{\downarrow}^{>}(t_{1},t_{2})K_{0\uparrow}(t_{1},t_{2})+\\
\mathrm{\int_{0}^{t}dt_{1}\int_{0}^{t^{\prime}}dt_{2}G_{\uparrow\downarrow}^{\dagger}(t^{\prime}-t_{2})G_{\uparrow\downarrow}(t-t_{1})\Delta_{\uparrow}^{>}(t_{1},t_{2})K_{0\downarrow}(t_{1},t_{2})}.
\end{multline*}

\end_inset

For equal times, 
\begin_inset Formula $K_{\alpha\beta}(t,t)$
\end_inset

 is the population probability on the dot.
 After initializing the vertex functions 
\begin_inset Formula $K_{\alpha\beta}(t,t^{\prime})$
\end_inset

 with 
\begin_inset Formula $K_{\alpha\beta}^{(0)}(t,t^{\prime})$
\end_inset

 and performing the self-consistent iteration sheme one is able to construct
 Greens functions in the following way 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{\alpha\uparrow}^{>}(t,t^{\prime})=-i\mathrm{\mathrm{\langle d_{\uparrow}(t)d_{\uparrow}^{\dagger}(t^{\prime})}\rangle}=K_{\alpha0}(t,t^{\prime})*G_{\uparrow}(t-t^{\prime})+K_{\alpha\downarrow}(t,t^{\prime})*G_{\uparrow\downarrow}(t-t^{\prime})
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{\alpha\uparrow}^{<}(t,t^{\prime})=i\mathrm{\mathrm{\langle d_{\uparrow}^{\dagger}(t^{\prime})d_{\uparrow}(t)}\rangle}=K_{\alpha\uparrow}(t,t^{\prime})*G_{0}(t-t^{\prime})+K_{\alpha\uparrow\downarrow}(t,t^{\prime})*G_{\downarrow}(t-t^{\prime})
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{\alpha\downarrow}^{>}(t,t^{\prime})=-i\mathrm{\mathrm{\langle d_{\downarrow}(t)d_{\downarrow}^{\dagger}(t^{\prime})}\rangle}=K_{\alpha0}(t,t^{\prime})*G_{\downarrow}(t-t^{\prime})+K_{\alpha\uparrow}(t,t^{\prime})*G_{\uparrow\downarrow}(t-t^{\prime})
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{\alpha\downarrow}^{<}(t,t^{\prime})=i\mathrm{\mathrm{\langle d_{\downarrow}^{\dagger}(t^{\prime})d_{\downarrow}(t)}\rangle=}K_{\alpha\downarrow}(t,t^{\prime})*G_{0}(t-t^{\prime})+K_{\alpha\uparrow\downarrow}(t,t^{\prime})*G_{\uparrow}(t-t^{\prime})
\]

\end_inset


\end_layout

\begin_layout Standard
with 
\begin_inset Formula $\alpha$
\end_inset

 denoting the initial dot state.
 
\end_layout

\begin_layout Itemize
